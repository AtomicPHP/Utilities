{
    "name": "niels-nijens/utilities",
    "type": "library",
    "description": "Utility classes",
    "keywords": [
        "Utilities"
    ],
    "homepage": "https://github.com/niels-nijens/Utilities",
    "license": "MIT",
    "authors": [
        {
            "name": "Niels Nijens",
            "email": "nijens.niels@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Nijens\\Utilities\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/Resources/mockclasses/"
        ],
        "psr-4": {
            "Nijens\\Utilities\\Tests\\": "tests/"
        }
    }
}
